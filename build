#!/bin/bash
cd /tmp/rom 

source build/envsetup.sh
export SELINUX_IGNORE_NEVERALLOWS=true
lunch cygnus_platina-userdebug
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 50G 
ccache -o compression=true 
ccache -z 

make sepolicy
make bootimage
make init
